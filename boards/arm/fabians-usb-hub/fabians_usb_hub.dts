/dts-v1/;

#include <st/f0/stm32f072Xb.dtsi>

/ {
	model = "Fabians USB Hub";
	compatible = "vendor,fabians-usb-hub";

	chosen {
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};

	leds {
		compatible = "gpio-leds";
		led0: led_0 {
			gpios = <&gpioa 6 GPIO_ACTIVE_HIGH>;
		};
	};

	buttons {
		compatible = "gpio-keys";
		button0: button_0 {
			gpios = <&gpioa 1 GPIO_ACTIVE_HIGH>;
			label = "Button";
		};
	};

	aliases {
		led0 = &led0;
	};

};


&clk_lsi {
	status = "okay";
};

&clk_hsi {
	status = "okay";
};
